<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColorHint">#e9e2d0</item>
        <item name="colorControlNormal">#393e46</item>
        <item name="colorControlActivated">#393e46</item>
        <item name="colorControlHighlight">#f9499e</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>


    <style name="Mytablayout" parent="Widget.Design.TabLayout">
        <item name="tabPaddingEnd">50dp</item>
        <item name="tabPaddingStart">50dp</item>
    </style>

    <style name="Translucent" parent="AppTheme">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>


    <style name="MyEditText" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">#fff9fa9b</item>
        <item name="colorControlActivated">#fff9fa9b</item>
    </style>
</resources>
